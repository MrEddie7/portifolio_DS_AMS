public class FormaA {
    public static void main(String[] args) {
        int linhas = 4;
        int colunas = 4;
        
        for (int i = 0; i < linhas; i++) {
            for (int j = 0; j < colunas; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}